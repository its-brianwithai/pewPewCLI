{
  "name": "pew-pew-cli",
  "version": "0.3.5",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pew": "bin/pew.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "node --loader ts-node/esm src/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js && npm run test:cucumber",
    "test:cucumber": "node --loader ts-node/esm ./node_modules/.bin/cucumber-js --import 'features/step_definitions/**/*.ts' --import 'features/support/**/*.ts' 'features/**/*.feature'"
  },
  "keywords": [
    "cli",
    "tasks",
    "workflow",
    "automation",
    "ai-agents",
    "cursor",
    "claude",
    "markdown",
    "todo",
    "agentic",
    "productivity",
    "ai-coding-assistant"
  ],
  "author": "ultrawideturbodev <brian@ultrawideturbodev.com>",
  "license": "SEE LICENSE IN LICENSE",
  "description": "pewPewCLI - agents fav dev tool",
  "files": [
    "dist",
    "bin",
    "README.md",
    "CHANGELOG.md"
  ],
  "private": false,
  "dependencies": {
    "clipboardy": "^4.0.0",
    "commander": "^13.1.0",
    "inquirer": "^12.5.2",
    "js-yaml": "^4.1.0",
    "latest-version": "^9.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.2.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
